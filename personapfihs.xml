        </Propiedad>
        <Propiedad ID="https://schema.org/extended/i_club" EntidadID="https://schema.org/Person">
            <AtrNombre xml:lang="es">Club</AtrNombre>
            <AtrNombreLectura xml:lang="es">Club</AtrNombreLectura>
            <AtrNombre xml:lang="en">Club</AtrNombre>
            <AtrNombreLectura xml:lang="en">Club</AtrNombreLectura>
            <SeleccionEntidad>
                <TipoSeleccion>Autocompletar</TipoSeleccion>
                <LinkARecurso IrAComunidad="true"></LinkARecurso>
                <Grafo>clubpfihs.owl</Grafo>
                <UrlTipoEntSolicitada>https://schema.org/SportsClub</UrlTipoEntSolicitada>
                <PropsEntEdicion>
                    <NameProp>https://schema.org/name</NameProp>
                </PropsEntEdicion>
                <PropsEntLectura>
                    <Propiedad ID="https://schema.org/name" EntidadID="https://schema.org/SportsClub">
                        <AtrNombre xml:lang="es">Nombre</AtrNombre>
                        <AtrNombreLectura xml:lang="es">Nombre</AtrNombreLectura>
                        <AtrNombre xml:lang="en">Name</AtrNombre>
                        <AtrNombreLectura xml:lang="en">Name</AtrNombreLectura>
                    </Propiedad>
                    <Propiedad ID="https://schema.org/logo" EntidadID="https://schema.org/SportsClub">
                        <AtrNombre>Logo</AtrNombre>
                        <AtrNombreLectura>Logo</AtrNombreLectura>
                        <MultiIdioma>false</MultiIdioma>
                        <TipoCampo>ImagenExterna</TipoCampo>
                    </Propiedad>
                </PropsEntLectura>
                <Reciproca></Reciproca>
                
                <ConsultaReciproca>
                    <![CDATA[SELECT DISTINCT(?club)
                    WHERE {
                    ?club a <https://schema.org/SportsClub>. 
                    ?club <https://schema.org/SportsTeam> ?equipo.
                     {
                        ?equipo <https://schema.org/athlete> ?persona.
                     } UNION {
                        ?equipo <https://schema.org/coach> ?persona.
                    }                    
                    FILTER (?persona = @EntidadID@)}]]>
                </ConsultaReciproca>
            </SeleccionEntidad>


        </Propiedad>


        <Propiedad ID="https://schema.org/nationality" EntidadID="https://schema.org/Person">
